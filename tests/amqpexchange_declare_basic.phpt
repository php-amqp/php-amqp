--TEST--
AMQPExchange
--SKIPIF--
<?php if (!extension_loaded("amqp")) print "skip"; ?>
--FILE--
<?php
$cnn = new AMQPConnection();
$cnn->connect();

$ch = new AMQPChannel($cnn);

$ex = new AMQPExchange($ch);
$ex->setName("exchange-" . time());
$ex->setType(AMQP_EX_TYPE_FANOUT);
echo $ex->declareExchange() ? "true" : "false";
?>
--EXPECT--
true
